{
  "name": "carlot-manager",
  "version": "1.0.0",
  "description": "Cloudflare Workers + Hono + Agents backend for managing car inventory, customers, and deals",
  "type": "module",
  "main": "src/worker.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:prod": "wrangler deploy --env production",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\" || echo \"lint optional\""
  },
  "dependencies": {
    "agents": "latest",
    "apt-get": "^0.1.0",
    "brew": "^0.0.8",
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250101.0",
    "eslint": "^9.0.0",
    "typescript": "^5.7.2",
    "wrangler": "^4.62.0"
  }
}
